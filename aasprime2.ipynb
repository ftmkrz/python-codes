   "source": [
    "n = int(input(\"Please enter a number: \"))\n",
    "list = []\n",
    "for num in range(1, n+1)\n",
    "    prime = True\n",
    "    for i in range(2, num):\n",
    "        if num % i == 0:\n",
    "            prime = False\n",
    "    if prime:\n",
    "        list.append(num)\n",
    "print(list)"
   ]
